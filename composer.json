{
    "name": "jsdecena/laracom",
    "description": "Laravel powered e-commerce",
    "keywords": ["e-commerce", "ecommerce", "shopping-cart", "laravel", "laracom", "laravel-ecommerce", "shop", "laravel5", "framework", "package"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "hardevine/shoppingcart": "^2.7",
        "jeroennoten/laravel-adminlte": "^3.0",
        "jsdecena/baserepo": "^1.0",
        "kalnoy/nestedset": "^5.0",
        "konekt/concord": "^1.4",
        "laravel/cashier": "^10.5",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "nicolaslopezj/searchable": "^1.12",
        "santigarcor/laratrust": "^5.2",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "binarytorch/larecipe": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "jsdecena/baserepo": "^1.0",
        "jsdecena/mailchimp": "~7.0",
        "jsdecena/mcpro": "1.1.*",
        "paypal/rest-api-sdk-php": "*",
        "shippo/shippo-php": "^1.4"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
